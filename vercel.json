{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/health.js", "use": "@vercel/node" },
    { "src": "api/handler.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/health", "dest": "/api/health.js" },
    { "src": "/api/(.*)", "dest": "/api/handler.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}